{
  "version": 2,
  "functions": {
    "backend/api/[...all].js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/[...all]"
    },
    {
      "source": "/weather/(.*)",
      "destination": "/backend/api/[...all]"
    },
    {
      "source": "/weather",
      "destination": "/backend/api/[...all]"
    },
    {
      "source": "/health",
      "destination": "/backend/api/[...all]"
    }
  ]
}
